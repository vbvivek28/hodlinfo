{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/tickers",
        "dest": "/app.js",
        "headers": {
            "Access-Control-Allow-Credentials": "true" ,
             "Access-Control-Allow-Origin": "https://hodlinfo-client.vercel.app" ,
            "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
         }
      }
    ]
  }
  